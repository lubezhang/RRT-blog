{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile-thinkjs": "babel --loose all --optional runtime --stage 0 --modules common src/ --out-dir app/ --retain-lines",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch-thinkjs": "npm run compile-thinkjs -- --watch",
    "watch-webpack": "webpack --watch",
    "watch": "npm run watch-thinkjs"
  },
  "dependencies": {
    "babel": "5.8.21",
    "babel-runtime": "5.6.17",
    "es6-promise": "^3.0.2",
    "isomorphic-fetch": "^2.2.0",
    "mongodb": "^2.1.0",
    "thinkjs": "2.0.x"
  },
  "devDependencies": {
    "babel-core": "^5.8.34",
    "babel-loader": "^5.4.0",
    "history": "^1.13.1",
    "marked": "^0.3.5",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.1.1",
    "redux-thunk": "^1.0.0",
    "webpack": "^1.12.9"
  }
}
