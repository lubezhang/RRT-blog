{
  "name": "My-Blog",
  "description": "My blog description",
  "version": "1.0.0",
  "main": "www/index.js",
  "author": "lubezhang",
  "license": "MIT",
  "scripts": {
    "start": "node www/development.js",
    "test": "npm run test-redux",
    "test:watch": "npm run test-redux -- --watch",
    "build": "npm run clear && npm run compile && npm run webpack",
    "test-server": "mocha test/server/**/*.test.js -t 5000 --reporter spec",
    "test-redux": "mocha test/redux/*.test.js -t 3000 --compilers js:babel-core/register",
    "clear": "rm -rf dist/server",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/server --out-dir dist/server/ --retain-lines",
    "webpack": "webpack --progress --colors --config config/webpack.config.production.js",
    "webpack-dev": "webpack --progress --colors --config config/webpack.config.develop.js",
    "static": "node dev-server.js"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "mysql": "^2.10.0",
    "thinkjs": "^2.1.5"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "6.x.x",
    "clean-webpack-plugin": "^0.1.6",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.7.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.0.0",
    "marked": "^0.3.5",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "node-sass": "^3.4.2",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.1.1",
    "redux-mock-store": "0.0.6",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^3.1.2",
    "should": "^8.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.0"
  }
}
